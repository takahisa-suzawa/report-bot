<div class="page-header">
  <%= link_to report_articles_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_report_article_path(@article), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show article</h1>
</div>

<dl class="dl-horizontal">
  <dt>Url:</dt>
  <dd><%= @article.url %></dd>

  <dt>Title:</dt>
  <dd><%= @article.title %></dd>

  <dt>Opening:</dt>
  <dd><%= @article.opening %></dd>

  <dt>Report:</dt>
  <dd><%= @article.Report %></dd>

</dl>
